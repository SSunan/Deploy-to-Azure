{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "<location>"	//i.e. canadacentral
        },
        "vnetName": {
            "value": "<vnetNam>"	//i.e. vnet9292
        },
        "vmName": {
            "value": "<vmName>"	//i.e. vmAdapterw19-01
        },
        "vmSize": {
            "value": "Standard_D4d_v4"	//i.e. Standard_D4d_v4
        },
        "vmUsername": {
            "value": "<vm admin user name>"	//i.e. vmadmin
        },
        "vmPassword": {
            "value": "<vm admin user password>"	//i.e. 
        },
        "nsgSourceAddressPrefix": {
            "value": "172.24.0.0/24" 	//i.e. 172.24.0.0/24
        },
        "sqlServerName": {
            "value": "<sql server name>"	//i.e. sqlAdapter-01
        },
        "sqlAdminUsername": {
            "value": "<sql server admin user name>"	//i.e. sqladmin
        },
        "sqlAdminPassword": {
            "value": "<sql server admin password>"	//i.e. 
        },
        "sqlDatabaseName": {
            "value": "<sql database name>"	//i.e. geotabadapterdb - match the Adapter document
        },
        "sqlDatabaseServiceTier": { //There are three SQL Database editions: Basic, Standard and Premium
            "value": "Standard"
        },
        "sqlDatabaseComputeSize": { //review https://docs.microsoft.com/en-us/azure/azure-sql/database/resource-limits-dtu-single-databases
            "value": "S1"
        },                
        "scriptUri": {
            "value": "https://github.com/SSunan/Deploy-to-Azure/releases/download/v1.0.0/Install_stuff.ps1"
        },
        "rootPath": {
            "value": "C:\\Geotab"
        },
        "appUser": {
            "value": "geotabadapter_client" //i.e. geotabadapter_client
        },
        "appPassword": {
            "value": "<app password>" //i.e. 
        },
        "loginMYGServer": {
            "value": "my.geotab.com" //i.e. my.geotab.com
        },
        "loginMYGDatabase": {
            "value": "<MyGeotabDatabase>" //i.e. bridge2020
        },
        "loginMYGUser": {
            "value": "<MyGeotabUser>" //i.e. 
        },
        "loginMYGPassword": {
            "value": "<MyGeotabPassword>" //i.e. 
        },
        "versionOfAdapter": {
            "value": "v1.5.0.0" //i.e. latest version of the Adapter on Github
            #"value": "v1.4.0.9"
        }                    
    }
}
